<table class="table table-bordered">
  <thead>
    <tr>
      <th>id</th>
      <th>user名</th>
      <th>email</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= link_to "delete", admin_user_path(user), method: :delete, data: { confirm: "reary?" }, class: "btn btn-primary" %></td>
      </tr>
    <% end %>
  </tbody>
</table>